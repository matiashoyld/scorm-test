{
  "version": 2,
  "buildCommand": "",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "api/token/route.js",
      "use": "@vercel/node"
    },
    {
      "src": "scormcontent/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/token",
      "dest": "/api/token/route"
    },
    {
      "src": "/",
      "dest": "/scormcontent/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/scormcontent/index.html"
    },
    {
      "src": "/lib/(.*)",
      "dest": "/scormcontent/lib/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/scormcontent/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/locales/(.*)",
      "dest": "/scormcontent/locales/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/scormcontent/$1"
    }
  ]
}
