{
  "version": 2,
  "buildCommand": "",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "api/token/route.js",
      "use": "@vercel/node"
    },
    {
      "src": "scormcontent/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/token",
      "dest": "/api/token/route"
    },
    {
      "src": "^/?$",
      "dest": "/scormcontent/index.html",
      "status": 308,
      "headers": {
        "Location": "/scormcontent/index.html"
      }
    },
    {
      "src": "/scormcontent/(.*)",
      "dest": "/scormcontent/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
