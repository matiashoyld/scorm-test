{
  "version": 2,
  "buildCommand": "",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "api/**/*",
      "use": "@vercel/node"
    },
    {
      "src": "scormcontent/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/token",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      },
      "continue": true
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/scormcontent/$1"
    }
  ]
}
